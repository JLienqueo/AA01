{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "useDefineForClassFields": false,
    "experimentalDecorators": true,
    "declaration": false,
    "module": "ESNext",
    "moduleResolution": "node",
    "target": "es2022",
    "strict": true,
     "skipLibCheck": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2022",
      "dom"
    ]
  }
}